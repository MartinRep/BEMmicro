entity Request {
	category String required,
    region String required,
    description String required,
    duration ZonedDateTime,
    expPrice Double,
    image ImageBlob,
    profile Integer,
    posted Instant
}

entity Offer {
	description String required,
    availableOn ZonedDateTime,
    price Double,
    profile Integer
}

relationship OneToMany {
	Request{offer(description)} to Offer{request(description)}
}
